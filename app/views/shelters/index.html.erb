<h5><%= link_to 'See All Pets', '/pets' %></h5>
<h3><%= link_to 'New Shelter', '/shelters/new' %></h3>
<p><% @shelters.each do |shelter| %>
  <section id="shelter-<%= shelter.id %>">
  <h2><%= link_to "#{shelter.name}", "/shelters/#{shelter.id}"%></h2>
  <ul>
  <%= link_to 'Edit Shelter', "/shelters/#{shelter.id}/edit"%>
  <%= link_to 'Delete Shelter', "/shelters/#{shelter.id}", method: :delete %>
  </section>
  </ul>
<% end %></p><br>
